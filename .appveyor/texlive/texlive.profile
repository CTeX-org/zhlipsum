# From latex3
# https://github.com/latex3/latex3/blob/master/support/texlive.profile

selected_scheme scheme-infraonly
TEXDIR %PROGRAMFILES%\texlive
TEXMFCONFIG %USERPROFILE%\.texlive\texmf-config
TEXMFHOME %USERPROFILE%\texmf
TEXMFLOCAL %PROGRAMFILES%\texlive\texmf-local
TEXMFSYSCONFIG %PROGRAMFILES%\texlive\texmf-config
TEXMFSYSVAR %PROGRAMFILES%\texlive\texmf-var
TEXMFVAR %USERPROFILE%\.texlive\texmf-var
option_doc 0
option_src 0
